<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.utkarsh.entity.Student" table="student1">

		<meta attribute="class-description">
			This class contains the employee detail.
		</meta>

		<id name="stRollNo" type="int" column="st_RollNO">
			<generator class="assigned" />
		</id>

		<property name="stName" column="st_Name" type="string" />
		<property name="stSection" column="st_Section" type="string" />
		<property name="stMarks" column="st_Marks" type="double" />

	</class>
</hibernate-mapping>