<?xml version="1.0" encoding="UTF-8"?>



<hibernate-mapping>
    <class name="com.utkarsh01.entity.EmployeeDetails" table="employee03">
        <!-- Primary key mapping -->
        <id name="empId" column="e_id">
            <generator class="increment" />
        </id>

        <property name="name" column="e_name" />
        <property name="dept" column="e_dept" />
        <property name="salary" column="e_sal" />
    </class>
</hibernate-mapping>
